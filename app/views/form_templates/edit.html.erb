<h1>Edit Form Template</h1>
<%= form_with model: @form_template do |f| %>
  <div>
    <%= f.label :carrier_id %>
    <%= f.number_field :carrier_id %>
  </div>
  <div>
    <%= f.label :customer_id %>
    <%= f.number_field :customer_id %>
  </div>
  <%= render "schema_editor", form_template: @form_template %>
  <%= f.submit %>
<% end %>
